{
  "name": "app",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "customizations": {
    "vscode": {
      "extensions": [
        "rebornix.Ruby",
        "wingrunr21.vscode-ruby",
        "ms-python.python"
      ]
    }
  },
  "forwardPorts": [3000],
  "postCreateCommand": "sudo service mysql start && bundle install && yarn install && pip install --user -r requirements.txt",
  "features": {
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.12.9"
    },
    "ghcr.io/devcontainers/features/git-lfs:1": {}
  },
  "remoteUser": "vscode"
}