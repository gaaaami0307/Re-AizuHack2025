{
  "name": "app",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "web",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "forwardPorts": [3000],
  "customizations": {
    "vscode": {
      "extensions": [
        "rebornix.Ruby",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "postCreateCommand": "bundle install && yarn install && bin/rails db:prepare"
}